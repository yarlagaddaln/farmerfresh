jQuery.fn.simplePopover = function(options) {
    var $ = jQuery;
	var defaults = {
			success: function() {},
			cancel: function() {}
	}
	if (!options) { options = {}};
	var settings = $.extend({}, defaults, options);
	var $alpha = $('#alphaLayer');
