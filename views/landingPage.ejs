    var pieces = oldUrl.split("_");
    var first = pieces[1];
    var num = 0.0;
    if(first == 'zero'){ num = 0.0;
    }else if(first == 'one'){ num = 1.0;
    }else if(first == 'two'){ num = 2.0;
    }else if(first == 'three'){ num = 3.0;
